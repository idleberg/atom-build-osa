{
  "name": "build-osa",
  "version": "0.9.1",
  "description": "Atom Build provider for Apple's Open Scripting Architecture, runs or compiles AppleScript and JavaScript for Automation (JXA)",
  "repository": "https://github.com/idleberg/atom-build-osa",
  "license": "MIT",
  "keywords": [
    "applescript",
    "buildprovider",
    "osx",
    "javascript for automation",
    "jxa"
  ],
  "main": "lib/provider.js",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "providedServices": {
    "builder": {
      "description": "Runs or compiles AppleScript and JXA",
      "versions": {
        "2.0.0": "provideBuilder"
      }
    }
  },
  "package-deps": [
    [
      {
        "name": "buildium"
      },
      {
        "name": "build"
      }
    ]
  ],
  "dependencies": {},
  "devDependencies": {
    "@atxm/developer-console": "^0.5.0",
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.3",
    "@babel/plugin-proposal-optional-chaining": "^7.14.2",
    "atom-package-deps": "^7.2.3",
    "atom-satisfy-dependencies": "^0.3.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.12.1",
    "eslint": "^7.26.0",
    "eslint-plugin-json": "^3.0.0",
    "husky": ">=4 <5",
    "lint-staged": "^11.0.0",
    "log-symbols": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "remark-cli": "^9.0.0",
    "rimraf": "^3.0.2"
  },
  "scripts": {
    "build": "node ./tools/build.mjs",
    "clean": "rimraf ./lib",
    "dev": "npm run start",
    "lint:md": "remark . --quiet --frail --ignore-path .gitignore",
    "lint:js": "eslint ./src --ignore-path .gitignore",
    "lint": "npm-run-all --parallel lint:*",
    "start": "cross-env ESBUILD_WATCH=true node ./tools/build.mjs",
    "test": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "volta": {
    "node": "14.17.0",
    "yarn": "1.22.10"
  }
}
